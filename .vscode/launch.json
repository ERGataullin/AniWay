    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "request": "launch",
            "type": "dart",
            "program": "app/lib/main.dart",
            "args": [
                "--web-browser-flag=--disable-web-security"
            ]
        },
        {
            "name": "Profile",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "app/lib/main.dart",
            "args": [
                "--web-browser-flag=--disable-web-security"
            ]
        },
        {
            "name": "Release",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "app/lib/main.dart",
            "args": [
                "--web-browser-flag=--disable-web-security"
            ]
        }
    ]
}
